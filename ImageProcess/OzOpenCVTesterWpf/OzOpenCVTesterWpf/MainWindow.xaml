        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Closing="Window_Closing"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Bottom">
            <Button Name="captureButton" Content="Start Capture" Click="OnClick캡춰시작"/>
            <Button Content="동영상열기" Click="OnClick동영상열기"/>
        </StackPanel>
        <StackPanel Name="후보리스트" Height="128" Orientation="Horizontal" DockPanel.Dock="Bottom">
        </StackPanel>
        <Image Name="img"></Image>
    </DockPanel>
</Window>
